{
  "name":           "(ORA@UMD) KR Extension for Power Users",
  "version":        "0.57",
  "description":    "A Chrome browser extension created by the Office of Research Administration at the University of Maryland.",
  "default_locale": "en",
  "background": {
    "persistent": true,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "ora_icon_off_128.png"
  },
  "permissions":    [
    "webNavigation",
    "activeTab",
    "https://*.kuali.co/*"

  ],
  "icons": {
    "128": "ora_icon_128.png"
  },  
  "manifest_version": 2
}
