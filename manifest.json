{
  "name":           "(ORA@UMD) KR Extension for Power Users",
  "version":        "1.00",
  "description":    "A Chrome browser extension created by the Office of Research Administration at the University of Maryland.",
  "default_locale": "en",
  "background": {
    "persistent": false,
    "scripts": [
      "configSettings.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "ora_icon_off_dark_128.png"
  },
  "permissions":    [
    "activeTab",
    "tabs",
    "https://*.kuali.co/res/*",
    "https://*.kuali.co/dashboard/*",
    "webNavigation",
    "https://*.kuali.co/res/*",
    "https://*.kuali.co/dashboard/*"
  ],
  "icons": {
    "128": "ora_icon_128.png"
  },
  "manifest_version": 2
}
